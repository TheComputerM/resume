{
  "name": "@thecomputerm/resume",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "cv": "bunx --bun resumed render cv.json --theme jsonresume-theme-graph-paper-grid -o docs/cv.html",
    "postcv": "sed -i '' 's/ - Present//g' ./docs/cv.html",
    "preresume": "bun run convert.ts",
    "resume": "uv run rendercv render rendercv.yaml -nopng -nomd --pdf-path ./docs/resume.pdf"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@resume/core": "npm:@jsonresume/core@^0.1.0",
    "jsonresume-theme-graph-paper-grid": "^1.0.0",
    "resumed": "^6.1.0"
  }
}
